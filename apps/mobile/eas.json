{
  "cli": {
    "version": ">= 12.6.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "build-and-maestro-test": {
      "withoutCredentials": true,
      "config": "build-and-maestro-test.yml",
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview",
        "EXPO_PUBLIC_WEB_URL": "https://atom-web-phi.vercel.app",
        "EXPO_PUBLIC_API_URL": "https://atom-web-phi.vercel.app/api"
      },
      "android": {
        "distribution": "internal",
        "withoutCredentials": true
      }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_WEB_URL": "https://atom-web-phi.vercel.app",
        "EXPO_PUBLIC_API_URL": "https://atom-web-phi.vercel.app/api"
      }
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "ios-simulator-preview": {
      "extends": "preview",
      "ios": {
        "simulator": true
      }
    },
    "base": {
    "ios": {
      "image": "macos-sonoma-14.6-xcode-16.1",
      "resourceClass": "large"
    }
  }
  },
  "submit": {
    "production": {}
  }
}
